        <menubar n="0">
            <default n="0">
                <menu n="100">
                    <label>777-300ER</label>
                    <name>payload</name>
                    <item n="3">
                        <label>Weight and payload</label>
                        <binding>
                            <command>dialog-show</command>
                            <dialog-name>payload</dialog-name>
                        </binding>
                    </item>
                </menu>
            </default>
        </menubar>

        <tutorials>
            <tutorial include="Tutorials/300/777-cockpit-overview.xml"/>
            <tutorial include="Tutorials/300/777-cockpit-pfd.xml"/>
            <tutorial include="Tutorials/300/777-cockpit-autopilot.xml"/>
            <tutorial include="Tutorials/300/777-preflight.xml"/>
            <tutorial include="Tutorials/300/777-pushback-startup.xml"/>
            <tutorial include="Tutorials/300/777-takeoff.xml"/>
            <!-- <tutorial include="Tutorials/300/777-approach-landing.xml"/> -->
        </tutorials>

        <weight n="1">
            <!--  365 pax max-->
            <name>Pax/Baggage</name>
            <weight-lb type="double">70420</weight-lb>
            <max-lb type="double">154000</max-lb>
            <min-lb type="double">0</min-lb>
        </weight>
        <weight n="2">
            <!--  Front Catering Galley -->
            <name>Front Catering Galley</name>
            <weight-lb type="double">0</weight-lb>
            <max-lb type="double">2325</max-lb>
            <min-lb type="double">0</min-lb>
        </weight>
        <weight n="3">
            <!--  Center Catering Galley -->
            <name>Center Catering Galley</name>
            <weight-lb type="double">0</weight-lb>
            <max-lb type="double">1850</max-lb>
            <min-lb type="double">0</min-lb>
        </weight>
        <weight n="4">
            <!--  Rear Catering Galley -->
            <name>Rear Catering Galley</name>
            <weight-lb type="double">0</weight-lb>
            <max-lb type="double">2675</max-lb>
            <min-lb type="double">0</min-lb>
        </weight>
        <aircraft-operator>JAL</aircraft-operator>
    </sim>

    <instrumentation n="0">
        <cdu n="0">
            <ident>
                <model>777-300ER</model>
                <engines>GE90-115B</engines>
            </ident>
        </cdu>
    </instrumentation>

    <autopilot n="0">
        <constant>
            <flare-base type="double">0.2</flare-base>
            <flch-thrust type="double">0.77</flch-thrust>
            <descent-profile-high-base type="double">0.19</descent-profile-high-base>
            <descent-profile-high-rate type="double">-0.00080214</descent-profile-high-rate>
            <descent-profile-low-base type="double">0.15</descent-profile-low-base>
            <descent-profile-low-rate type="double">-0.00026738</descent-profile-low-rate>
        </constant>
    </autopilot>
    
    <aircraft>
     <jetway-pos>
      <x-tra>-1.8</x-tra>
      <y-rot>3.3</y-rot>
      <z-rot>18.3</z-rot>
      <hood-rot>-6</hood-rot>
      <!--z-entrance-rotation>26</z-entrance-rotation-->
     </jetway-pos>
     <jetway2-pos>
      <x-tra>2.8</x-tra>
      <y-rot>8.0</y-rot>
      <z-rot>48</z-rot>
      <hood-rot>-5</hood-rot>
      <!--z-entrance-rotation>49</z-entrance-rotation-->
     </jetway2-pos>
    </aircraft>
    
    <services>
        <payload>
            <first-max-nr>22</first-max-nr>
            <business-max-nr>70</business-max-nr>
            <economy-max-nr>273</economy-max-nr>
            <catering-max-lbs>6850</catering-max-lbs>
        </payload>
    </services>

</PropertyList>
