<PropertyList include="777-set-common.xml">

    <sim n="0">
        <description>Boeing 777-300ER</description>
        <aero>777-300ER</aero>
        <max-payload type="double">408060</max-payload>
        <max-landing-weight type="double">554000</max-landing-weight>
        <max-takeoff-weight type="double">775000</max-takeoff-weight>
        <startup>
            <splash-texture>Aircraft/777/gui/Previews/splash-300ER.png</splash-texture>
        </startup>

        <variant-of>777-300</variant-of>

        <model n="0">
            <path>Aircraft/777/Models/777-300ER.xml</path>
            <door>
                <position-x-m type="float">-23.85</position-x-m>
                <position-y-m type="float"> -2.95</position-y-m>
                <position-z-m type="float"> -0.55</position-z-m>
                <jetway-hood-deg type="float">3</jetway-hood-deg>
            </door>
            <door>
                <position-x-m type="float">-13.49</position-x-m>
                <position-y-m type="float"> -2.95</position-y-m>
                <position-z-m type="float"> -0.55</position-z-m>
                <jetway-hood-deg type="float">3</jetway-hood-deg>
            </door>
            <ramp>
                <x-m type="float">-24.5221</x-m>
                <class type="string">0</class>
            </ramp>
        </model>
        
        <view n="0">
