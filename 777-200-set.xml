       <menubar n="0">
            <default n="0">
                <menu n="100">
                    <label>777-200</label>
					<item n="3">
					    <label>Weight and payload</label>
                        <name>payload</name>
						<binding>
                            <command>dialog-show</command>
							<dialog-name>payload</dialog-name>
                        </binding>
                    </item>
                </menu>
            </default>
        </menubar>

        <tutorials>
            <tutorial include="Tutorials/200/777-cockpit-overview.xml"/>
            <tutorial include="Tutorials/200/777-cockpit-pfd.xml"/>
            <tutorial include="Tutorials/200/777-cockpit-autopilot.xml"/>
            <tutorial include="Tutorials/200/777-preflight.xml"/>
            <tutorial include="Tutorials/200/777-pushback-startup.xml"/>
            <tutorial include="Tutorials/200/777-takeoff.xml"/>
            <!-- <tutorial include="Tutorials/200/777-approach-landing.xml"/> -->
        </tutorials>

         <weight n="1">
            <!--  328 pax max @ 180 lbs + cargo -->
            <name>Pax/Baggage</name>
            <weight-lb type="double">44840</weight-lb>
            <max-lb type="double">125530</max-lb>
            <min-lb type="double">0</min-lb>
        </weight>
        <aircraft-operator>BAW</aircraft-operator>
    </sim>

    <instrumentation n="0">
        <cdu n="0">
            <ident n="0">
                <model>777-200</model>
                <engines>RR Trent 877</engines>
            </ident>
        </cdu>
    </instrumentation>

    <autopilot n="0">
        <constant>
            <flare-base type="double">0.315</flare-base>
            <flch-thrust type="double">0.85</flch-thrust>
            <descent-profile-high-base type="double">0.13</descent-profile-high-base>
            <descent-profile-high-rate type="double">-0.00094203</descent-profile-high-rate>
            <descent-profile-low-base type="double">0.12</descent-profile-low-base>
            <descent-profile-low-rate type="double">-0.00050725</descent-profile-low-rate>
        </constant>
    </autopilot>
	<aircraft>
	 <jetway-pos>
      <x-tra>-1.8</x-tra>
	  <y-rot>3.3</y-rot>
	  <z-rot>18.3</z-rot>
	  <hood-rot>-6</hood-rot>
	  <!--z-entrance-rotation>26</z-entrance-rotation-->
	 </jetway-pos>
	 <jetway2-pos>
	  <x-tra>2.8</x-tra>
	  <y-rot>8.0</y-rot>
	  <z-rot>48</z-rot>
	  <hood-rot>-5</hood-rot>
	  <!--z-entrance-rotation>49</z-entrance-rotation-->
     </jetway2-pos>
    </aircraft>
	
	<services> <!--Max nr of passengers in each class. Data from https://en.wikipedia.org/wiki/Boeing_777#Specifications-->
	    <payload>
		    <first-max-nr>24</first-max-nr>
			<business-max-nr>54</business-max-nr>
			<economy-max-nr>227</economy-max-nr>
		</payload>
	</services>
	
</PropertyList>
