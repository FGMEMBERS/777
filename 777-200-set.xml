<PropertyList include="777-set-common.xml">

    <sim n="0">
        <description>Boeing 777-200</description>
        <aero>777-200</aero>
        <max-payload type="double">238000</max-payload>
        <max-landing-weight type="double">445000</max-landing-weight>
		<max-takeoff-weight type="double">545000</max-takeoff-weight>
        <startup>
            <splash-texture>Aircraft/777/gui/Previews/splash-200.png</splash-texture>
        </startup>
        <primary-set type="bool">true</primary-set>
        <model n="0">
            <path>Aircraft/777/Models/777-200.xml</path>
            <door>
                <position-x-m type="float">-18.30</position-x-m>
                <position-y-m type="float"> -2.95</position-y-m>
                <position-z-m type="float"> -0.42</position-z-m>
                <jetway-hood-deg type="float">3</jetway-hood-deg>
            </door>
            <door>
                <position-x-m type="float">-7.67</position-x-m>
                <position-y-m type="float"> -2.95</position-y-m>
                <position-z-m type="float"> -0.42</position-z-m>
                <jetway-hood-deg type="float">3</jetway-hood-deg>
            </door>
			<ramp>
	            <x-m type="float">-19.284</x-m>
                <class type="string">1</class>
            </ramp>
        </model>
        <view n="0">
            <config n="0">
                <z-offset-m type="double">-22.84</z-offset-m>
            </config>
        </view>
        <view n="100">
            <config n="0">
                <z-offset-m type="double">-22.84</z-offset-m>
