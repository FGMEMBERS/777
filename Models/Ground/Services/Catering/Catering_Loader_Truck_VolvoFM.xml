<?xml version="1.0"?>
<PropertyList>

	<path>Catering_Loader_Truck_VolvoFM.ac</path>
	
	<animation>
        <type>pick</type>
		<object-name>cater_bridge</object-name>
		<object-name>gourmet_cargo</object-name>
		<object-name>gourmet_railing</object-name>
		<object-name>gourmet_scissor_inner</object-name>
		<object-name>gourmet_scissor_outer</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-toggle</command>
                <property>services/catering/move</property>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
		<object-name>cater_truck</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-toggle</command>
                <property>services/catering/enable</property>
            </binding>
        </action>
    </animation>	
	
	<animation>
		<type>translate</type>
		<object>cater_bridge</object>
		<object>cater_truck</object>
		<object>gourmet_cargo</object>
		<object>gourmet_railing</object>
		<object>gourmet_scissor_inner</object>
		<object>gourmet_scissor_outer</object>
		<property>sim/model/ramp/x-m</property>
		<offset-m>24.5221</offset-m>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>cater_bridge</object-name>
		<property>services/catering/position</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0.0</dep>
			</entry>
			<entry>
				<ind>0.47368421052</ind>
				<dep>0.0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>2.0</dep>
			</entry>
		</interpolation> 
		<axis>
			<z>1</z>
		</axis>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>gourmet_railing</object-name>
		<object-name>gourmet_cargo</object-name>
		<property>services/catering/position</property>
		<factor>3.8</factor>
		<axis>
			<z>1</z>
		</axis>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>gourmet_scissor_outer</object-name>
		<object-name>gourmet_scissor_inner</object-name>
		<property>services/catering/position</property>
		<factor>1.9</factor>
		<axis>
			<z>1</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>gourmet_scissor_outer</object-name>
		<property>services/catering/position</property>
		<factor>32</factor>
		<expression>
		    <rad2deg>
				<asin>
					<div>
						<product>
							<property>services/catering/position</property>
							<value>3.8</value>
						</product>
						<value>7.36481</value>
					</div>
				</asin>
			</rad2deg>
		</expression>
		<center>
			<x-m>-12.66448</x-m>
			<y-m>9.15734</y-m>
			<z-m>-4.46443</z-m>
		</center>
		<axis>
		    <x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>gourmet_scissor_inner</object-name>
		<property>services/catering/position</property>
		<factor>32</factor>
		<expression>
		    <rad2deg>
				<asin>
					<div>
						<product>
							<property>services/catering/position</property>
							<value>-3.8</value>
						</product>
						<value>7.36481</value>
					</div>
				</asin>
			</rad2deg>
		</expression>
		<center>
			<x-m>-12.66448</x-m>
			<y-m>9.15734</y-m>
			<z-m>-4.46443</z-m>
		</center>
		<axis>
		    <x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
<animation>
	<type>translate</type>
	<object>cater_bridge</object>
	<object>cater_truck</object>
	<object>gourmet_cargo</object>
	<object>gourmet_railing</object>
	<object>gourmet_scissor_inner</object>
	<object>gourmet_scissor_outer</object>
	<property>gear/gear[0]/compression-norm</property>
	<interpolation>
		<entry><ind>0.15</ind><dep>0.0</dep></entry>
		<entry><ind>0.40</ind><dep>0.15</dep></entry>
	</interpolation>
	<axis>
		<x>0</x>
		<y>0</y>
		<z>1</z>
	</axis>
</animation>

</PropertyList>
