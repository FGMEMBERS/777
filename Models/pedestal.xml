            <x-m>0.193</x-m>
            <y-m>0</y-m>
            <z-m>0.401</z-m>
        </center>
    </animation>

	<model>
        <name>Radio1</name>
        <path>Aircraft/777/Models/Instruments/radio1.xml</path>
