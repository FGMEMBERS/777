<PropertyList>

 <!-- =============================================================== -->
 <!-- Lateral                                                         -->
 <!-- =============================================================== -->

	<filter>
		<name>TO/GA</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1</gain>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>TO/GA</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<value>0</value>
		</input>
		<output>/autopilot/internal/target-roll-deg</output>
	</filter>
	
	<filter>
		<name>ROLLOUT-flare-aileron</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1</gain>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>ROLLOUT</value>
					</equals>
					<or>
						<equals>
							<property>/gear/gear[1]/wow</property>
							<value>0</value>
						</equals>
						<equals>
							<property>/gear/gear[2]/wow</property>
							<value>0</value>
						</equals>
					</or>
				</and>
			</condition>
		</enable>
		<input>0.0</input>
		<output>/autopilot/internal/target-roll-deg</output>
	</filter>
	
	<filter>
		<name>ROLLOUT-aileron</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1</gain>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>ROLLOUT</value>
					</equals>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<property>/controls/flight/rudder</property>
			<scale>-1.25</scale>
		</input>
		<output>/controls/flight/aileron-ap</output>
	</filter>
	
	<filter>
		<name>HEADING ERROR DEG</name>
		<debug>false</debug>
		<type>gain</type>
		<input>
			<condition>
				<or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>HDG SEL</value>
					</equals>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>HDG HOLD</value>
					</equals>
				</or>
			</condition>
			<property>/autopilot/settings/heading-bug-deg</property>
			<offset>
				<property>/orientation/heading-magnetic-deg</property>
				<scale>-1.0</scale>
			</offset>
		</input>
		<input>
			<condition>
				<or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>TRK SEL</value>
					</equals>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>TRK HOLD</value>
					</equals>
				</or>
			</condition>
			<property>/autopilot/settings/heading-bug-deg</property>
			<offset>
				<property>/orientation/heading-deg</property>
				<scale>-1.0</scale>
			</offset>
		</input>
		<input>
			<condition>
				<and>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>HDG SEL</value>
					</not-equals>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>HDG HOLD</value>
					</not-equals>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>TRK SEL</value>
					</not-equals>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>TRK HOLD</value>
					</not-equals>
				</and>
			</condition>
			<value>0</value>
		</input>
		<output>/autopilot/internal/heading-error-deg</output>
		<period>
			<min>-180</min>
			<max>180</max>
		</period>
		<gain>1.0</gain>
	</filter>

	<pi-simple-controller>
		<name>HDG HOLD</name>
		<debug>false</debug>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<or>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>HDG SEL</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>HDG HOLD</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>TRK SEL</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/roll-mode</property>
							<value>TRK HOLD</value>
						</equals>
					</or>
				</and>
			</condition>
		</enable>
		<input>
			<property>/autopilot/internal/heading-error-deg</property>
			<scale>1.2</scale>
		</input>
		<reference>
			<value>0</value>
		</reference>
		<output>
			<property>/autopilot/internal/target-roll-deg</property>
		</output>
		<config>
			<Kp>-1.0</Kp>
			<Ki>0.00</Ki>
			<u_min>
				<property>/instrumentation/afds/settings/bank-min</property>
			</u_min>
			<u_max>
				<property>/instrumentation/afds/settings/bank-max</property>
			</u_max>
		</config>
	</pi-simple-controller>

	<pi-simple-controller>
		<name>LNAV1</name>
		<debug>false</debug>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>LNAV</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<property>/autopilot/internal/course-error-deg</property>
		</input>
		<reference>
			<value>0.0</value>
		</reference>
		<output>
			<property>/autopilot/internal/target-roll-deg</property>
		</output>
		<config>
			<Kp>-1.75</Kp>
			<Ki>0.00001</Ki>
			<u_min>
				<property>/instrumentation/afds/settings/bank-min</property>
			</u_min>
			<u_max>
				<property>/instrumentation/afds/settings/bank-max</property>
			</u_max>
		</config>
	</pi-simple-controller>

	<filter>
		<name>nav1 heading error computer/normalizer</name>
		<debug>false</debug>
		<type>gain</type>
		<input>
			<property>/instrumentation/nav[0]/radials/target-auto-hdg-deg</property>
			<offset>
				<property>/orientation/heading-deg</property>
				<scale>-1.0</scale>
			</offset>
		</input>
		<output>/autopilot/internal/nav1-heading-error-deg</output>
		<period>
			<min>-180</min>
			<max>180</max>
		</period>
		<gain>1.0</gain>
	</filter>

	<pi-simple-controller>
		<name>LOC1</name>
		<debug>false</debug>
		<enable>
			<condition>
				<and>
					<or>
						<property>/instrumentation/afds/inputs/AP</property>
						<property>/instrumentation/afds/inputs/FD</property>
					</or>
					<equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>LOC</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<property>/autopilot/internal/nav1-heading-error-deg</property>
		</input>
		<reference>
			<value>0.0</value>
		</reference>
		<output>
			<property>/autopilot/internal/target-roll-deg</property>
		</output>
		<config>
			<Kp>-2.0</Kp>
			<Ki>-0.0001</Ki>
			<u_min>
				<property>/instrumentation/afds/settings/bank-min</property>
			</u_min>
			<u_max>
				<property>/instrumentation/afds/settings/bank-max</property>
			</u_max>
		</config>
	</pi-simple-controller>
	
	<filter>
		<name>ROLL DEG SYNC</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1</gain>
		<enable>
			<condition>
				<and>
					<not><property>/instrumentation/afds/inputs/AP</property></not>
					<not><property>/instrumentation/afds/inputs/FD</property></not>
				</and>
			</condition>
		</enable>
		<input>orientation/roll-deg</input>
		<output>/autopilot/internal/target-roll-deg</output>
		<min>
			<property>/instrumentation/afds/settings/bank-min</property>
		</min>
		<max>
			<property>/instrumentation/afds/settings/bank-max</property>
		</max>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: TARGET ROLL CMD</name>
		<debug>false</debug>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>
			<property>/autopilot/internal/target-roll-deg</property>
		</input>
		<output>/autopilot/internal/target-roll</output>
		<type>noise-spike</type>
		<max-rate-of-change>6</max-rate-of-change>
	</filter>
	
	<filter>
		<name>System Command: Roll Difference</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>-0.1</gain>
		<enable>
			<condition>
				<property>/instrumentation/afds/inputs/AP</property>
			</condition>
		</enable>
		<input>
			<property>/orientation/roll-deg</property>
		</input>
		<reference>
			<property>/autopilot/internal/target-roll</property>
		</reference>
		<output>
			<property>/autopilot/internal/rate-aileron-cmd</property>
		</output>
		<min>-0.4</min>
		<max>0.4</max>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: ROLL RATE CMD</name>
		<type>gain</type>
		<gain>1</gain>
		<input>
			<expression>
				<table>
					<property>/autopilot/internal/rate-aileron-cmd</property> 
					<entry><ind>-1</ind><dep>-12.0</dep></entry>
					<entry><ind> 0</ind><dep>  0.0</dep></entry>
					<entry><ind> 1</ind><dep> 12.0</dep></entry>
				</table>
			</expression>
		</input>
		<output>/autopilot/internal/roll-rate</output>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: ROLL RATE KP (P Gain)</name>
		<type>gain</type>
		<gain>1</gain>
		<input>
			<expression>
				<table>
					<property>/velocities/mach</property>
					<entry><ind>0.2</ind><dep>0.16</dep></entry>
					<entry><ind>0.9</ind><dep>0.06</dep></entry>
				</table>
			</expression>
		</input>
		<output>/autopilot/internal/roll/kp</output>
	</filter>
	
	<pid-controller>
		<name>System Command: Roll Rate</name>
		<debug>false</debug>
		<enable>
			<condition>
				<and>
					<property>/instrumentation/afds/inputs/AP</property>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/roll-mode</property>
						<value>ROLLOUT</value>
					</not-equals>
					<or>
						<equals>
							<property>/gear/gear[1]/wow</property>
							<value>0</value>
						</equals>
						<equals>
							<property>/gear/gear[2]/wow</property>
							<value>0</value>
						</equals>
					</or>
				</and>
			</condition>
		</enable>
		<input>
			<property>/orientation/roll-rate-degps</property>
		</input>
		<reference>
			<property>/autopilot/internal/roll-rate</property>
		</reference>
		<output>
			<property>/controls/flight/aileron-ap</property>
		</output>
		<config>
			<Kp>
				<property>/autopilot/internal/roll/kp</property>
			</Kp>
			<beta>1</beta>
			<alpha>0.1</alpha>
			<gamma>0</gamma>
			<Ti>1.0</Ti>
			<Td>0.0001</Td>
			<u_min>-1</u_min>
			<u_max>1</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>System Command: Aileron</name>
		<debug>false</debug>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<enable>
			<condition>
				<property>/instrumentation/afds/inputs/AP</property>
			</condition>
		</enable>
		<input>
			<property>/controls/flight/aileron-ap</property>
		</input>
		<output>/controls/flight/aileron</output>
		<type>noise-spike</type>
		<max-rate-of-change>0.5</max-rate-of-change>
	</filter>

 <!-- =============================================================== -->
 <!-- Vertical                                                        -->
 <!-- =============================================================== -->
	
	<predict-simple>
		<name>ALTITUDE 5 SECONDS AHEAD</name>
		<debug>false</debug>
		<input>instrumentation/altimeter/indicated-altitude-ft</input>
		<output>/autopilot/internal/altitude-5-sec-ahead</output>
		<seconds>5.0</seconds>
		<filter-gain>0.0</filter-gain>
	</predict-simple>
  
	<filter>
		<name>ALTITUDE CAPTURE/HOLD</name>
		<debug>false</debug>
		<type>gain</type>
		<input>
			<expression>
				<sum>
					<property>/autopilot/internal/altitude-5-sec-ahead</property>
					<value>-5</value>
				</sum>
			</expression>
		</input>
		<reference>
			<property>/autopilot/settings/actual-target-altitude-ft</property>
		</reference>
		<gain>-8</gain>
		<output>
			<property>/autopilot/internal/target-fpm-alt</property>
		</output>
		<min>-2000</min>
		<max>2000</max>
	</filter>
	
	<predict-simple>
		<name>IAS 15 SECOND PREDICTOR</name>
		<debug>false</debug>
		<input>instrumentation/airspeed-indicator/indicated-speed-kt</input>
		<output>/autopilot/internal/lookahead-15-sec-airspeed-kt</output>
		<seconds>15.0</seconds>
		<filter-gain>0.0</filter-gain>
	</predict-simple>
	
	<predict-simple>
		<name>MACH 15 SECOND PREDICTOR</name>
		<debug>false</debug>
		<input>instrumentation/airspeed-indicator/indicated-mach</input>
		<output>/autopilot/internal/lookahead-15-sec-mach</output>
		<seconds>15.0</seconds>
		<filter-gain>0.0</filter-gain>
	</predict-simple>
	
	<filter>
		<name>KTS INPUT FILTER</name>
		<debug>false</debug>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>instrumentation/afds/ias-target-kt</input>
		<output>/autopilot/internal/flch-kts</output>
		<type>noise-spike</type>
		<max-rate-of-change>
			<condition>
				<or>
					<and>
						<not-equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>TO/GA</value>
						</not-equals>
						<not-equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>FLCH SPD</value>
						</not-equals>
					</and>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>1</value>
					</equals>
				</or>
			</condition>
			<value>100</value>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<and>
					<or>
						<equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>TO/GA</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>FLCH SPD</value>
						</equals>
					</or>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<value>5</value>
		</max-rate-of-change>
	</filter>
	
	<filter>
		<name>MACH INPUT FILTER</name>
		<debug>false</debug>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>autopilot/settings/target-speed-mach</input>
		<output>/autopilot/internal/flch-mach</output>
		<type>noise-spike</type>
		<max-rate-of-change>
			<condition>
				<or>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>FLCH SPD</value>
					</not-equals>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>1</value>
					</equals>
				</or>
			</condition>
			<value>1</value>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<and>
					<equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>FLCH SPD</value>
					</equals>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<value>0.05</value>
		</max-rate-of-change>
	</filter>
	
	<pid-controller>
		<name>FLCH SPEED BY PITCH</name>
		<debug>false</debug>
		<enable>
			<condition>
				<or>
					<equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>TO/GA</value>
					</equals>
					<equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>FLCH SPD</value>
					</equals>
					<equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>VNAV SPD</value>
					</equals>
					<and>
						<equals>
							<property>/instrumentation/afds/ap-modes/pitch-mode</property>
							<value>VNAV PTH</value>
						</equals>
						<equals>
							<property>/instrumentation/afds/inputs/vnav-path-mode</property>
							<value>2</value>
						</equals>
					</and>
				</or>
			</condition>
		</enable>
		<input>
			<condition>
				<equals>
					<property>/instrumentation/afds/inputs/ias-mach-selected</property>
					<value>0</value>
				</equals>
			</condition>
			<property>/autopilot/internal/lookahead-15-sec-airspeed-kt</property>
		</input>
		<input>
			<condition>
				<equals>
					<property>/instrumentation/afds/inputs/ias-mach-selected</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/autopilot/internal/lookahead-15-sec-mach</property>
			<scale>500.0</scale>
		</input>
		<reference>
			<condition>
				<equals>
					<property>/instrumentation/afds/ap-modes/pitch-mode</property>
					<value>TO/GA</value>
				</equals>
			</condition>
			<expression>
				<sum>
					<property>/instrumentation/weu/state/v2</property>
					<value>10</value>
				</sum>
			</expression>
		</reference>
		<reference>
			<condition>
				<and>
					<equals>
						<property>/instrumentation/afds/inputs/ias-mach-selected</property>
						<value>0</value>
					</equals>
					<not-equals>
						<property>/instrumentation/afds/ap-modes/pitch-mode</property>
						<value>TO/GA</value>
					</not-equals>
				</and>
			</condition>
			<property>/autopilot/internal/flch-kts</property>
		</reference>
		<reference>
			<condition>
				<equals>
					<property>/instrumentation/afds/inputs/ias-mach-selected</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/autopilot/internal/flch-mach</property>
			<scale>500.0</scale>
		</reference>
		<output>
			<property>/autopilot/internal/target-fpm-spd</property>
		</output>
		<config>
			<Kp>-50</Kp>
			<beta>1.0</beta>    
			<alpha>0.1</alpha>  
