<?xml version="1.0"?>

<PropertyList>

    <filter>
        <name>Fuel fraction calculation</name> 
        <type>gain</type>
        <update-interval-secs type="double">0.05</update-interval-secs>
        <gain>1.0</gain>
        <input>
            <property>consumables/fuel/total-fuel-norm</property>
        </input>
        <output>sim/fuel-fraction</output>
    </filter>

</PropertyList>
